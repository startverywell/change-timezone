{
  "name": "time-zone-converter",
  "version": "3.0.0",
  "description": "A Chrome extension that converts times to preferred Time Zone",
  "main": "main.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "predev": "npm test",
    "dev": "npx webpack --env.NODE_ENV=local",
    "prebuild": "npm-run-all test lint",
    "build": "npx webpack --env.NODE_ENV=production",
    "lint": "esw webpack.config.* src libs --color"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richardaspinall/chrome-timezone-converter.git"
  },
  "author": "Richard Aspinall",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/richardaspinall/chrome-timezone-converter/issues"
  },
  "homepage": "https://github.com/richardaspinall/chrome-timezone-converter#readme",
  "dependencies": {
    "moment-locales-webpack-plugin": "^1.1.0",
    "moment-timezone": "^0.5.26"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "7.13.8",
    "@babel/node": "^7.13.0",
    "@babel/preset-env": "7.13.8",
    "@babel/register": "7.13.8",
    "babel-loader": "8.2.2",
    "css-loader": "^5.2.0",
    "eslint": "7.21.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-watch": "7.0.0",
    "file-loader": "^6.2.0",
    "mocha": "^8.3.2",
    "npm-run-all": "^4.1.5",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.2",
    "webpack": "^5.30.0",
    "webpack-cli": "^3.3.12"
  }
}
